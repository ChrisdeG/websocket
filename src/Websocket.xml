<?xml version="1.0" encoding="utf-8"?>
<widget id="chrisdegelder.websocket.Websocket" pluginWidget="true" needsEntityContext="true" offlineCapable="true"
        supportedPlatform="Web"
        xmlns="http://www.mendix.com/widget/1.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.mendix.com/widget/1.0/ ../node_modules/mendix/custom_widget.xsd">
    <name>Websocket</name>
    <description>Listens to websocket messages</description>
    <icon>
 
    </icon>
    <properties>
        <propertyGroup caption="General">
            <property key="subscriptionAttribute" type="attribute" required="true">
                <caption>Subscription attribute</caption>
                <description>The attribute that contains the subscription id</description>
                <attributeTypes>
                    <attributeType name="String"/>
                </attributeTypes>
            </property>
            <property key="progressAttribute" type="attribute" required="false">
                <caption>Progress attribute</caption>
                <description>The attribute that contains progress</description>
                <attributeTypes>
                    <attributeType name="Integer"/>
                    <attributeType name="Long"/>
                </attributeTypes>
            </property>            
            <property key="text1Attribute" type="attribute" required="false">
                <caption>Text attribute 1</caption>
                <description>An attribute that contains text</description>
                <attributeTypes>
                    <attributeType name="String"/>
                </attributeTypes>
            </property>            
            <property key="text2Attribute" type="attribute" required="false">
                <caption>Text attribute 3</caption>
                <description>An attribute that contains text</description>
                <attributeTypes>
                    <attributeType name="String"/>
                </attributeTypes>
            </property>            
            <property key="text3Attribute" type="attribute" required="false">
                <caption>Text attribute 3</caption>
                <description>An attribute that contains text</description>
                <attributeTypes>
                    <attributeType name="String"/>
                    <attributeType name="Enum"/>
                </attributeTypes>
            </property>            
            <property key="booleanAttribute" type="attribute" required="false">
                <caption>Boolean attribute</caption>
                <description>An attribute that contains a boolean</description>
                <attributeTypes>
                    <attributeType name="Boolean"/>
                </attributeTypes>
            </property>            
            <property key="decimalAttribute" type="attribute" required="false">
                <caption>Decimal attribute</caption>
                <description>An attribute that contains a decimal</description>
                <attributeTypes>
                    <attributeType name="Decimal"/>
                </attributeTypes>
            </property>            
            <property key="dateAttribute" type="attribute" required="false">
                <caption>Date attribute </caption>
                <description>An attribute that contains a date</description>
                <attributeTypes>
                    <attributeType name="DateTime"/>
                </attributeTypes>
            </property>            
        </propertyGroup>
        <propertyGroup caption="Events">
            <property key="onUpdateAction" type="action" required="false">
                <caption>On receive update</caption>
                <description>Action to trigger when a message is received. Nanoflow is preferred for performance.</description>
            </property>
        </propertyGroup>
        <propertyGroup caption="Settings">
            <property key="path" type="string" required="true">
                <caption>Path</caption>
                <description>Should be the same as the constant WebsocketName, don't start with /</description>
            </property>
        </propertyGroup>        
    </properties>
</widget>
